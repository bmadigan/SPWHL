<%= form_for @team_schedule do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :team_id %><br />
    <%= f.text_field :team_id %>
  </p>
  <p>
    <%= f.label :event_date %><br />
    <%= f.datetime_select :event_date %>
  </p>
  <p>
    <%= f.label :event_title %><br />
    <%= f.text_field :event_title %>
  </p>
  <p>
    <%= f.label :event_content %><br />
    <%= f.text_area :event_content %>
  </p>
  <p><%= f.submit %></p>
<% end %>
