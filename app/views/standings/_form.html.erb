<%= form_for @standings do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :league_id %><br />
    <%= f.text_field :league_id %>
  </p>
  <p>
    <%= f.label :game_id %><br />
    <%= f.text_field :game_id %>
  </p>
  <p>
    <%= f.label :home_team_score %><br />
    <%= f.text_field :home_team_score %>
  </p>
  <p>
    <%= f.label :away_team_score %><br />
    <%= f.text_field :away_team_score %>
  </p>
  <p><%= f.submit %></p>
<% end %>
